<% if flash[:notice] %>
    <%= flash[:notice] %>
<% end %>

<% @twitters.each do |tweet| %>
    <p>
        <%= tweet.message %>
        <%= tweet.created_at %>
        <%= link_to '削除', "/tweets/#{tweet.id}", method: :delete %>
        <%= link_to '表示', "/tweets/#{tweet.id}" %>
        <%= link_to '編集', "/tweets/#{tweet.id}/edit" %>
    </p>
<% end %>
<p><%= link_to '新規登録', '/tweets/new' %></p>